		GradeSchool, HighSchool, Associates, Bachelors, Masters, Doctorate
	}
	
	public Person()
	{
		int random = (int) (Math.random() * 1001);
		
	}
	
	public void drive()
	{
		
	}
	
	public String toString()
	{
		return "[" + age + ", " +  gender + ", " + education + ", ";
	}
	
	
}
