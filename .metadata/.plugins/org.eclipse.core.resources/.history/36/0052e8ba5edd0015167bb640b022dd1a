			if(random > ageRegionProbabilities[i] && random > ageRegionProbabilities[i+1])
			{
				ageRegion = i;
				break;
			}
		}
		System.out.println(ageRegion);
		
		
		for(int i = 0; i < genderRegions.length - 1; i++)
